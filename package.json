    "mobx-react": "6.1.5",
    "next": "9.2.1",
    "p-waterfall": "2.1.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "semantic-ui-css": "2.4.1",
    "semantic-ui-react": "0.88.2",
    "sjs-base-model": "1.9.0",
    "uuid": "3.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.8.3",
