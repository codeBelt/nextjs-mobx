{
  "name": "nextjs-mobx",
  "description": "",
  "version": "1.0.0",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "post-commit": "git update-index -g"
    }
  },
  "scripts": {
    "---------- Helpers ------------------------------------------------------": "",
    "ts": "tsc --noEmit",
    "ts:watch": "npm run ts -- --watch",
    "prettier": "prettier --write \"./**/*.{ts,tsx,js,jsx,json,md}\"",
    "---------- Development --------------------------------------------------": "",
    "storybook": "start-storybook -p 6006",
    "storybook-build": "build-storybook -o ./docs",
    "dev": "cross-env CLIENT_ENV=development next",
    "---------- Production ---------------------------------------------------": "",
    "build": "cross-env CLIENT_ENV=production next build",
    "start": "cross-env CLIENT_ENV=production next start",
    "export": "cross-env CLIENT_ENV=production next build && next export",
    "---------- Analyzing ----------------------------------------------------": "",
    "analyze": "cross-env ANALYZE=true npm run build",
    "eslint": "eslint '*/**/*.{js,jsx,ts,tsx}'",
    "eslint:fix": "npm run eslint --fix",
    "lint": "npm-run-all --parallel eslint ts",
    "---------- Testing ------------------------------------------------------": "",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "-------------------------------------------------------------------------": ""
  },
